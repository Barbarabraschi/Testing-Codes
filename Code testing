SELECT * FROM students
WHERE country = 'Poland' AND age = '18'
ORDER BY names DESC;

UPDATE students
SET city = 'Warsaw'
WHERE school = 'CentrumSchool';

DELETE from students 
WHERE school = 'WolaSchool';


SELECT MIN(weight) AS Lowestweight
FROM clients
WHERE weight BETWEEN 60 AND 80
ORDER BY weightloss;

SELECT *
FROM clients
WHERE weight IS NULL;
